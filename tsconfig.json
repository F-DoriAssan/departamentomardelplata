{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],

    "baseUrl": ".", 
    "paths": {
  "@/app/*": ["src/app/*"]
}
  },
  "include": [
    ".env.local.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/[locale]/lib/placeholder-data.js",
    "scripts/seed.js"
, "src/app/[locale]/page.jsx", "src/app/[locale]/page.jsx", "i18nConfig.js"  ],
  "exclude": ["node_modules"]
}
